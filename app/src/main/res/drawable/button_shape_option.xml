<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="25dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#666465"
        android:startColor="#919191"
        android:endColor="#0A0909"
        android:type="linear"
        />
    <padding
        android:left="10dp"
        android:top="0dp"
        android:right="10dp"
        android:bottom="0dp"
        />
    <size
        android:width="60dp"
        android:height="30dp"
        />
    <stroke
        android:width="3dp"
        android:color="#878787"
        />
</shape>